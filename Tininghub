tab.newToggle("Anti void", "Antivoid Void protect", true, function(toggleState)
    if toggleState then
        local block = Instance.new("Part")
        block.Name = "AntiVoid"
        block.Size = Vector3.new(1000, 1, 3000) 
        block.Transparency = 0.5 
        block.CanCollide = true 
        block.Anchored = true 
        block.Position = Vector3.new(1, -9, 0)
        block.Parent = game.Workspace
    else
        local block = game.Workspace:FindFirstChild("AntiVoid")
        if block then
            block:Destroy()
        end
    end
end)

tab.newToggle("Anti ragdoll", "Anti ragdoll prevents you Ragdoll", true, function(toggleState)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local torso = character:WaitForChild("Torso")
    local humanoid = character:WaitForChild("Humanoid")
    local debounce = false

    local function anchorTorso()
        if not debounce and humanoid.Health > 0 then
            debounce = true
            torso.Anchored = true
            task.wait(0.5)
            torso.Anchored = false
            debounce = false
        end
    end

    if toggleState then
        humanoid:GetPropertyChangedSignal("PlatformStand"):Connect(function()
            if humanoid.PlatformStand then
                anchorTorso()
            end
        end)

        player.CharacterAdded:Connect(function(newCharacter)
            character = newCharacter
            torso = newCharacter:WaitForChild("Torso")
            humanoid = newCharacter:WaitForChild("Humanoid")

            humanoid:GetPropertyChangedSignal("PlatformStand"):Connect(function()
                if humanoid.PlatformStand then
                    task.wait(2)
                    anchorTorso()
                end
            end)
        end)
    else
        humanoid:GetPropertyChangedSignal("PlatformStand"):Disconnect()
        player.CharacterAdded:Disconnect()
    end
end)

tab.newToggle("Disable Cube of Death", "Disables cube of death kill when touched", true, function(toggleState)
    local cube = workspace.Arena.CubeOfDeathArea["the cube of death(i heard it kills)"]
    
    if toggleState then
        cube.CanTouch = false
    else
        cube.CanTouch = true
    end
end)

tab.newToggle("Anti pusher", "Disables Pusher ability wall", true, function(toggleState)
    for _, v in pairs(game.Workspace:GetChildren()) do
        if v.Name == "wall" then
            if toggleState then
                v.CanCollide = false
            else
                v.CanCollide = true
            end
        end
    end
end)

tab.newToggle("Anti stun", "Disable Stun's Ragdoll", true, function(toggleState)
    local player = game.Players.LocalPlayer
    local character = player.Character
    local humanoid = character and character:FindFirstChild("Humanoid")
    local shockwave = game.Workspace:FindFirstChild("Shockwave")
    local ragdolled = character and character:FindFirstChild("Ragdolled")

    if humanoid and shockwave and ragdolled and ragdolled.Value == false then
        if toggleState then
            humanoid.PlatformStand = false
        else
            humanoid.PlatformStand = true
        end
    end
end)

