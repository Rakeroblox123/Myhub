tab.newButton("Get [REDACTED]", "Automatically gets glove [REDACTED] (Requires 5k slaps)", function()
    local player = game.Players.LocalPlayer
    local slaps = player.leaderstats and player.leaderstats.Slaps

    if slaps and slaps.Value >= 5000 then
        local Door = 0
        for i = 1, 10 do
            Door = Door + 1
            if not game:GetService("BadgeService"):UserHasBadgeAsync(player.UserId, 2124847850) then
                firetouchinterest(player.Character:WaitForChild("Head"), workspace.PocketDimension.Doors[Door], 0)
                firetouchinterest(player.Character:WaitForChild("Head"), workspace.PocketDimension.Doors[Door], 1)
                wait(3.75)
            end
        end
    else
        print("You need at least 5000 slaps to use this!")
    end
end)

