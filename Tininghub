local tab = DrRayLibrary.newTab("Badge/Gloves", "ImageIdHere")

local baseplateCreated = false

tab.newButton("Teleport Safe", "Teleports Safe Area", function()
    local antiVoidPosition = Vector3.new(190, 50, -13095)

    local function createAntiVoidBarrier(position)
        local antiVoidPart = Instance.new("Part")
        antiVoidPart.Size = Vector3.new(512, 1, 512)
        antiVoidPart.Position = position
        antiVoidPart.Anchored = true
        antiVoidPart.Transparency = 0.5
        antiVoidPart.BrickColor = BrickColor.new("Bright blue")
        antiVoidPart.CanCollide = true
        antiVoidPart.Parent = game.Workspace
    end

    local function teleportPlayer(position)
        local player = game.Players.LocalPlayer
        if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local abovePosition = position + Vector3.new(0, 3, 0)
            player.Character.HumanoidRootPart.CFrame = CFrame.new(abovePosition)
        end
    end

    if not baseplateCreated then
        createAntiVoidBarrier(antiVoidPosition)
        baseplateCreated = true
    end

    teleportPlayer(antiVoidPosition)
end)

